<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>welcome  @Context.Session.GetString("userName")</h1> 
    @{
    @if(@Context.Session.GetInt32("userId") == 2){
    <h3> you are the @Context.Session.GetInt32("userId")nd person  to be added</h3>
    }
    @if(@Context.Session.GetInt32("userId") == 3){
    <h3> you are the @Context.Session.GetInt32("userId")rd person  to be added</h3>
    }
    @if(@Context.Session.GetInt32("userId") != 2 || @Context.Session.GetInt32("userId") !=3)
    {
        <h3> you are the @Context.Session.GetInt32("userId")th person  to be added</h3>
    }
    }
    

    <form action="/logout" method="post" > <button> logout </button></form> 
</body>
</html>